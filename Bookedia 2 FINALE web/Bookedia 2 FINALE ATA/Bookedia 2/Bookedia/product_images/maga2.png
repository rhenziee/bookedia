	<div class="login-forgot__content">
		<!-- Step 1, where user enters his email -->
		<div class="login-forgot__step-1 active">
			<div class="login-body__title">Forgot your password?</div>
			<div class="login-form__wrapper">
				<form class="login-form" method="post" data-hs-cf-bound="true">
					<label for="emailInputForgot">Enter your email to receive activation code.</label>
					<input type="email" class="form-control js-validate-email" id="emailInputForgot" placeholder="Enter Email" pattern=".*" required="" aria-label="Enter your email to receive activation code." data-uw-rm-form="nfx">
					<button id="buttonForgot" class="btn btn-lg btn-primary btn-block js-submit-form" disabled="disabled" type="submit" name="Reset Password">Reset Password</button>
					<div class="login-forgot__notice">
						<span>*</span>
						<span>Please note that this form cannot be used to reset your Google <br role="presentation" data-uw-rm-sr="">or Facebook password. Visit <a href="https://accounts.google.com/" data-uw-rm-brl="PR" data-uw-original-href="https://accounts.google.com/">Google</a> or <a href="https://www.facebook.com/" data-uw-rm-brl="PR" data-uw-original-href="https://www.facebook.com/">Facebook</a> to do that.</span>
					</div>
				</form>
			</div>
		</div>

		<!-- Step 2, where user enters 6-digit code, which comes to his email -->
		<div class="login-forgot__step-2">
			<div class="login-body__title">Reset your password</div>
			<div class="login-form__wrapper">
				<form class="login-form recovery-code-form" method="post" data-hs-cf-bound="true">
					<label for="codeInputReset">An email has been sent to <span id="restoreEmail"></span> with a<br role="presentation" data-uw-rm-sr=""> recovery code. Please enter it below:</label>
					<div class="recovery-code__wrapper">
						<input type="number" min="0" max="9" maxlength="1" class="recovery-code__input" autocomplete="off" aria-label="Validation Code" data-uw-rm-form="nfx">
						<input type="number" min="0" max="9" maxlength="1" class="recovery-code__input" autocomplete="off" aria-label="Validation Code" data-uw-rm-form="nfx">
						<input type="number" min="0" max="9" maxlength="1" class="recovery-code__input" autocomplete="off" aria-label="Validation Code" data-uw-rm-form="nfx">
						<input type="number" min="0" max="9" maxlength="1" class="recovery-code__input" autocomplete="off" aria-label="Validation Code" data-uw-rm-form="nfx">
						<input type="number" min="0" max="9" maxlength="1" class="recovery-code__input" autocomplete="off" aria-label="Validation Code" data-uw-rm-form="nfx">
						<input type="number" min="0" max="9" maxlength="1" class="recovery-code__input" autocomplete="off" aria-label="Validation Code" data-uw-rm-form="nfx">
					</div>
					<button class="btn btn-lg btn-primary btn-block js-submit-form" type="submit" id="buttonRecoveryCode" disabled="disabled" name="Submit">Proceed</button>
				</form>
				<button class="resend-code-btn" id="resend-code-btn"><span>Resend code</span></button>
			</div>
		</div>

		<!-- User enters his new password -->
		<div class="login-forgot__step-3">
			<div class="login-body__title">Enter new password</div>
			<form class="login-form new-password-form" method="post" data-hs-cf-bound="true">
				<label for="confirmPasswordReset" class="screen-reader-only">Enter new password</label>
				<div class="password-input-wrapper">
					<input type="password" class="form-control js-validate-password js-password" id="passwordReset" placeholder="Enter Password" pattern=".*" required="" aria-label="Password" data-uw-rm-form="nfx">
					<i class="view-password-button" tabindex="0" aria-live="assertive" role="button" name="Show or hide password" aria-pressed="false" onclick="inline.utility.showPassword( &#39;reset&#39; )" data-uw-rm-kbnav="click" aria-label="button" data-uw-rm-empty-ctrl=""></i>
				</div>
				<label for="confirmPasswordReset" class="screen-reader-only">Confirm Password</label>
				<input type="password" class="form-control js-password" id="confirmPasswordReset" placeholder="Confirm Password" pattern=".*" required="" aria-label="Confirm entered password" data-uw-rm-form="nfx">

				<div class="password-requirements">
					<p class="password-requirements__title">Your password must include:</p>
					<ul class="password-requirements__list">
						<li class="password-requirements__list-item pswd-length">Min 8 characters</li>
						<li class="password-requirements__list-item pswd-lowercase">Min 1 lowercase character</li>
						<li class="password-requirements__list-item pswd-uppercase">Min 1 uppercase character</li>
						<li class="password-requirements__list-item pswd-number">Min 1 number</li>
					</ul>
				</div>
				<button id="buttonRestore" class="btn btn-lg btn-primary btn-block js-submit-form" type="submit" disabled="disabled" name="Submit">Submit</button>
			</form>
		</div>

		<span class="already-have-acc">Or get back to the <a id="signin-go-back" class="signin-go-back" role="button" name="Sign In" tabindex="0">Sign In</a></span>
	</div>
